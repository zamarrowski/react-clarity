{
  "name": "react-clarity",
  "version": "0.4.0",
  "description": "React clarity",
  "main": "lib/index.js",
  "author": "zamarrowski",
  "license": "ISC",
  "dependencies": {
    "prop-types": "15.5.10",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-ionicons": "2.1.6",
    "styled-components": "2.2.3"
  },
  "scripts": {
    "prepublish": "babel ./src --out-dir ./lib --source-maps --presets es2015,react,stage-2 --plugins babel-plugin-add-module-exports && cp src/MetropolisRegular.otf ./lib",
    "start": "webpack --watch",
    "build": "webpack",
    "copyToDocs": "bash copyFilesToDocs.sh",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:test": "wait-on http-get://localhost:6006 && npm run cypress:run",
    "test": "concurrently 'npm run storybook' 'npm run cypress:test' -k -s first"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.4.10",
    "@storybook/addon-knobs": "3.4.10",
    "@storybook/addon-links": "3.4.10",
    "@storybook/addons": "3.4.10",
    "@storybook/react": "3.4.10",
    "babel": "6.23.0",
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-loader": "6.3.2",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-2": "6.22.0",
    "babel-runtime": "6.26.0",
    "concurrently": "^4.1.0",
    "css-loader": "0.26.1",
    "cypress": "^3.1.5",
    "file-loader": "0.10.1",
    "jsx-loader": "0.13.2",
    "storybook-readme": "4.0.5",
    "style-loader": "0.13.1",
    "wait-on": "^3.2.0",
    "webpack": "2.2.1"
  }
}
